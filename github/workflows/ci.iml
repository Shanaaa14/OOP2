name: PHP CI Pipeline
on:
    push:
        branches:
        - main

jobs:
    build-test:
    runs-on: ubuntu-latest

    steps:
    - name: Checkout repository
    uses: actions/checkout@v3

    - name: Setup PHPuses: shivammathur/setup-php@v2with:
    php-version:"8.1"

    -name: Run PHP Script
    run: php index.php

    -name: Run Test
    run: php test.php